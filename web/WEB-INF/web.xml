<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    
    <servlet>
        <servlet-name>Signup</servlet-name>
        <servlet-class>Signup</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>Profile</servlet-name>
        <servlet-class>Profile</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>login</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>champs</servlet-name>
        <servlet-class>champs</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>champs</servlet-name>
        <url-pattern>/champs</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>findSum</servlet-name>
        <servlet-class>findSum</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>recentSearch</servlet-name>
        <servlet-class>recentSearch</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>results</servlet-name>
        <servlet-class>results</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>champResult</servlet-name>
        <servlet-class>champResult</servlet-class>
    </servlet>


    
    <servlet-mapping>
        <servlet-name>Signup</servlet-name>
        <url-pattern>/Signup</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>Profile</servlet-name>
        <url-pattern>/Profile</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>findSum</servlet-name>
        <url-pattern>/findSum</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>recentSearch</servlet-name>
        <url-pattern>/recentSearch</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>results</servlet-name>
        <url-pattern>/results</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>champResult</servlet-name>
        <url-pattern>/champResult</url-pattern>
    </servlet-mapping>
</web-app>